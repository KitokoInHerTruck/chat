<div id="<%= dom_id event %>">


  <p>
    <strong>Intitulé du concours:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>
  
  <p>
    <strong>Date du concours:</strong>
    <%= event.start_date %>
  </p>

  <p>
    <strong>Nombre de jours:</strong>
    <%= event.duration %>
  </p>

  <p>
    <strong>Prix de l'engagement en €:</strong>
    <%= event.price %>
  </p>

  <p>
    <strong>Ville:</strong>
    <%= event.location %>
  </p>

</div>
